<form autocomplete="off">
  <select class="sidebar-projectVersionsDropdown">
    {{#each nodes}}
      <option translate="no" value="{{url}}"{{#if isCurrentVersion}} selected disabled{{/if}}>
        {{version}}
      </option>
    {{/each}}
  </select>
  {{#each nodes}}
    {{#if latest}}
      {{#unless isCurrentVersion}}
        <button type="button" class="sidebar-projectLatestVersionButton" value="{{url}}">Jump to Latest</button>
      {{/unless}}
    {{/if}}
  {{/each}}
</form>
